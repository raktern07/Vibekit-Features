    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.5.0",
    "a2a-samples-js": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "viem": "^2.26.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.5.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^3.2.1"
  }
