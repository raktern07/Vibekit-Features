# Inference API keys
OPENAI_API_KEY=sk-placeholder-key
OPENROUTER_API_KEY=

# The gRPC endpoint for Ember API. For local testing, you might use localhost:50051.
EMBER_ENDPOINT=grpc.api.emberai.xyz:50051

# EVM RPC URL
RPC_URL=https://arbitrum.llamarpc.com

# QuickNode RPC Endpoint Configuration
# Subdomain for your QuickNode endpoint (e.g., omniscient-capable-wave)
QUICKNODE_SUBDOMAIN="your_quicknode_subdomain_here"
QUICKNODE_API_KEY="your_quicknode_api_key_here"

# Enable token caching to improve startup performance
AGENT_CACHE_TOKENS=false

# Timeout in milliseconds for calls to the MCP server (e.g., getCapabilities)
# Defaults to 30000 (30 seconds) if not set
# MCP_TOOL_TIMEOUT_MS=90000 