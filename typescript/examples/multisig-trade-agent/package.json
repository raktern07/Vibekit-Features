    "ai": "^4.0.29",
    "arbitrum-vibekit-core": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ember-schemas": "workspace:*",
    "express": "^4.21.2",
    "viem": "^2.26.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.5.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^3.2.1"
