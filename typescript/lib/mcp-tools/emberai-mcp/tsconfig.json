{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "Node16",
    "outDir": "dist",
    "rootDir": "./src",
    "composite": true,
    "paths": {
      "ember-schemas": ["../../ember-schemas/src/index.js"],
      "ember-schemas/*": ["../../ember-schemas/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"],
  "references": [{ "path": "../../ember-schemas" }]
}
